---
import MainLayout from "../layout/MainLayout.astro";
import WeatherComponent from "../components/WeatherComponent.astro";
import IndexArticle from "../components/IndexArticle.astro";
const title = "Plate Cove, Newfoundland & Labrador";
import "../styles/styles.css";
import heroImage from "../assets/hero_images/view1.png";
---

<MainLayout title={title} backgroundImage={heroImage}>
	<main>
		
		<section class="grid grid-cols-1 md:grid-cols-1 lg:grid-cols-4 text-black">
			<IndexArticle />
			<WeatherComponent />
		</section>
	</main>
</MainLayout>