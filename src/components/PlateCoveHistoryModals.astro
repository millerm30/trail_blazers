import plateCoveHistorySeventeen from "../assets/history/oldPhoto3.jpeg";
import plateCoveHistoryEighteen from "../assets/history/oldPhoto4.jpeg";
---

<Modal 
  modalId={"modal-1"} 
  headingText={"Sawmill"} 
  paragraphText={"Keough family mill towards the point located in Plate Cove West. Dated - 1984"} 
  imgSource={plateCoveHistoryOne} 
  imgAlt={"History Image 1"} 
/>
<Modal 
  modalId={"modal-2"} 
  headingText={"Slip Repairs"} 
  paragraphText={"Stripping logs for repairs to slipway located in Plate Cove East. Dated - 1984"} 
  imgSource={plateCoveHistoryTwo} 
  imgAlt={"History Image 2"} 
/>
<Modal 
  modalId={"modal-3"} 
  headingText={"Slipway repairs"} 
  paragraphText={"Stripping Logs for repairs to slipway located in Plate Cove West. Dated - 1984"} 
  imgSource={plateCoveHistoryThree} 
  imgAlt={"History Image 3"} 
/>
<Modal 
  modalId={"modal-4"} 
  headingText={"Warf repair discussion"} 
  paragraphText={"Bill Mews (left) & Bill Walsh in Plate Cove West. Dated - 1984"} 
  imgSource={plateCoveHistoryFour} 
  imgAlt={"History Image 4"} 
/>
<Modal 
  modalId={"modal-5"} 
  headingText={"Boat Launch 1960's"} 
  paragraphText={"Daniel Keough's motorboat being launched on Bayview Drive(formely known as the back lane) in the 1960's - Plate Cove West."}
  photoName={"Wife - Ellen (braker) Keough upper middle of picture in white dress. Genevieve Furlong to the right of Ellen Keough (4th. to the right in a dress)"}
