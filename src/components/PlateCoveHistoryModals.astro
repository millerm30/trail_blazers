import plateCoveHistoryThree from "../assets/history/platecoveHistory4.jpg";
import plateCoveHistoryFour from "../assets/history/platecoveHistory5.jpg";
import plateCoveHistoryFive from "../assets/history/boatPhoto.jpg";
import plateCoveHistorySix from "../assets/history/oldWarf.jpg";
import plateCoveHistorySeven from "../assets/history/platecoveHistory2.jpg";
import plateCoveHistoryEight from "../assets/history/platecoveHistory6.jpg";
import plateCoveHistoryNine from "../assets/history/platecoveHistory7.jpg";
import plateCoveHistoryTen from "../assets/history/platecoveHistory8.jpg";
import plateCoveHistoryEleven from "../assets/history/platecoveHistory9.jpg";
import plateCoveHistoryTwelve from "../assets/history/platecoveHistory10.jpg";
---

<Modal 
  modalId={"modal-1"} 
  headingText={"Plate Cove West "} 
  paragraphText={"Keough family mill."} 
  imgSource={plateCoveHistoryOne} 
  imgAlt={"Bill Mews & Bill Walsh"} 
/>
<Modal 
  modalId={"modal-2"} 
  headingText={"Slip Repairs"} 
  paragraphText={"Stripping logs for repairs to slipway - Plate Cove East "} 
  imgSource={plateCoveHistoryTwo} imgAlt={"Image Alt"} 
/>
<Modal 
  modalId={"modal-3"} 
  headingText={"Plate Cove West"} 
  paragraphText={"Stripping Logs for repairs to slipway"} 
  imgSource={plateCoveHistoryThree} imgAlt={"Image Alt"} 
/>
<Modal 
  modalId={"modal-4"} 
  headingText={"Warf Repairs"} 
  paragraphText={"Bill Mews (left) & Bill Walsh"} 
  imgSource={plateCoveHistoryFour} 
  imgAlt={"Bill Mews * Bill Walsh"} 
/>
<Modal 
  modalId={"modal-5"} 
  headingText={"Boat Launch 1960's"} 
  paragraphText={"Daniel Keough's motorboat being launched in the Bayview Drive(formely known as the back lane) in the 1960's."}
  photoName={"Wife - Ellen (braker) Keough upper middle of picture in white dress. Genevieve Furlong to the right of Ellen Keough (4th. to the right in a dress)"}
  imgSource={plateCoveHistoryFive} imgAlt={"Image Alt"}
/>
<Modal 
  modalId={"modal-6"} 
  headingText={"Plate Cove West - 1950's"} 
  paragraphText={"Thomas (Mr. Tommy) Keough's warf & shed from the 1950's"} 
  photoName={"Upper left corner by ocean is Patrick (Mr. Pat) Walsh's Warf. Upper right corner Patrick (Mr. Pat) Walsh's house & shed on Walsh's side."}
  imgSource={plateCoveHistorySix} imgAlt={"Image Alt"} 
/>
