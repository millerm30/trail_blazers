  imgSource={plateCoveHistorySix}
   imgAlt={"History Image 6"} 
/>
<Modal 
  modalId={"modal-7"} 
  headingText={"Bald Rock Point"} 
  paragraphText={"As you enter from Plate Cove West. Dated - 1984 Plate Cove East"} 
  imgSource={plateCoveHistorySeven} 
  imgAlt={"History Image 7"} 
/>
