---
import Modal from "../components/Modal.astro";
import plateCoveHistoryFourteen from "../assets/history/oldPhoto.jpeg";
import plateCoveHistoryFifteen from "../assets/history/oldPhoto1.jpeg";
import plateCoveHistorySixteen from "../assets/history/oldPhoto2.jpeg";
import plateCoveHistorySeventeen from "../assets/history/oldPhoto3.jpeg";
import plateCoveHistoryEighteen from "../assets/history/oldPhoto4.jpeg";
---

<Modal 
  modalId={"modal-1"} 
  headingText={"History"} 
  paragraphText={"Mary (Philpott) Hyde, Mary (Heaney) Keough and Mary (Whalen) Keough. Plate Cove West."} 
  imgSource={plateCoveHistoryFourteen} 
  imgAlt={"History Image 14"}
/>
<Modal 
  modalId={"modal-2"} 
  headingText={"A man and his horse"} 
  paragraphText={"Herbet Keough and his horse. Plate Cove West"} 
  imgSource={plateCoveHistoryFifteen} 
  imgAlt={"History Image 15"}
/>
<Modal 
  modalId={"modal-3"} 
  headingText={"Family Photo"} 
  paragraphText={"From letf to right. Terry Keough, Mary Ellen (Keough) Heaney, Fred Keough, (next 4 people unknown). Top left to right is Mary (Heaney) Keough and Ellen (Braker) Keough. Plate Cove West."} 
  imgSource={plateCoveHistorySixteen} 
  imgAlt={"History Image 16"}
/>
<Modal 
  modalId={"modal-4"} 
  headingText={"History"} 
  paragraphText={"Ellen (Braker) Keough in Plate Cove West."} 
  imgSource={plateCoveHistorySeventeen} 
  imgAlt={"History Image 17"}
/>
<Modal 
  modalId={"modal-5"} 
  headingText={"Horse Ride"} 
  paragraphText={"Herbert Keough and Fred Keough enjoying a horse ride in Plate Cove West."} 
  imgSource={plateCoveHistoryEighteen} 
